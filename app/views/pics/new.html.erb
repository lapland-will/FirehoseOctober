
<div class="text-center my-wrapper stolen-surfbox">


<h1 class="text-center customfont3">SHOW ME WHAT YOU'VE GOT</h1>

<br/>
<div class="span4 offset5 text-center stolen-surfbox">

<%= simple_form_for @pic do |f| %>
	<%= f.input :hour, :lable=>'HOUR' %>
	<%= f.input :emotion, :lable=>'MOOD'%>
	<%= f.input :learned, :lable=>'LEARNED' %>
	<%= submit_tag "Save My DooDoo", :class => 'btn btn-success' %>
</div>

<% end %>

</div>